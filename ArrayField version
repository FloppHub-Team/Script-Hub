local Tab4 = Window:CreateTab("MISC", 4483362458)
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local speedEnabled = false
local defaultSpeed = 16
local maxSpeed = 1000
local jumpSlider
local defaultJumpPower = 50
local maxJumpPower = 1000

local function setPlayerSpeed(speed)
    if humanoid then
        humanoid.WalkSpeed = speed
    end
end

Tab4:CreateToggle({
    Name = "MODIFICAR VELOCIDAD",
    CurrentValue = false,
    Flag = "SpeedToggle",
    Callback = function(value)
        speedEnabled = value
        if not speedEnabled then
            setPlayerSpeed(defaultSpeed)
        end
    end
})

Tab4:CreateSlider({
    Name = "SPEED",
    Range = {defaultSpeed, maxSpeed},
    Increment = 1,
    Suffix = " Speed",
    CurrentValue = defaultSpeed,
    Flag = "SpeedSlider",
    Callback = function(value)
        if speedEnabled then
            setPlayerSpeed(value)
        end
    end
})

Tab4:CreateToggle({
    Name = "CAMBIAR PODER DEL SALTO",
    Default = false,
    Callback = function(state)
        if humanoid then
            if state then
                if not jumpSlider then
                    jumpSlider = Tab4:CreateSlider({
                        Name = "🦘┃PODER DE SALTO",
                        Range = {1, maxJumpPower},
                        Increment = 1,
                        CurrentValue = humanoid.JumpPower,
                        Suffix = " JumpPower",
                        Callback = function(value)
                            humanoid.JumpPower = value
                        end
                    })
                end
            else
                humanoid.JumpPower = defaultJumpPower
                if jumpSlider then
                    jumpSlider:Destroy() -- Asegúrate de eliminar el slider
                    jumpSlider = nil -- Restablece la variable para evitar múltiples instancias
                end
            end
        end
    end
})

Tab4:CreateButton({
    Name = "FLY JUMP",
    Info = {
        Description = generateDescription("FLY JUMP")
    },
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/FloppHub-Team/Fly-Jump/refs/heads/main/Script",true))()
    end
})

Tab4:CreateToggle({
    Name = "SALTOS INFINITOS",
    Info = {
        Description = "ACTIVAR/DESACTIVAR LOS SALTOS INFINITOS"
    },
    CurrentValue = false,
    Callback = function(state)
        if state then
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
            game.Players.LocalPlayer.Character.Humanoid.AutoRotate = true
            while state do
                wait(0.1)
                if game.Players.LocalPlayer.Character.Humanoid.Jump then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 50, 0)
                end
            end
        else
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
            game.Players.LocalPlayer.Character.Humanoid.AutoRotate = true
        end
    end
})

Tab4:CreateToggle({
    Name = "LOOP FULL BRIGHT",
    Info = {
        Description = "ACTIVAR PARA QUE SIEMPRE SEA DE DIA"
    },
    CurrentValue = false,
    Callback = function(Value)
        if Value then
            game:GetService("RunService").Stepped:Connect(function()
                game.Lighting.ClockTime = 14
            end)
        end
    end
})

Tab4:CreateToggle({
    Name = "NO LIMIT ZOOM",
    Info = {
        Description = "ALEJAR LA CAMARA INFINITAMENTE"
    },
    CurrentValue = false,
    Callback = function(Value)
        if Value then
            while Value do
                game.Players.LocalPlayer.CameraMaxZoomDistance = game.Players.LocalPlayer.CameraMaxZoomDistance + 999999999999999999999999999999999999999999999999999999
                wait(0.1)
            end
        else
            game.Players.LocalPlayer.CameraMaxZoomDistance = OriginalMaxZoomDistance
        end
    end
})

