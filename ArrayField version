local ArrayField = loadstring(game:HttpGet("https://raw.githubusercontent.com/FloppHub-Team/Script-Hub/refs/heads/main/Cracked%20Library.txt"))()

local Window = ArrayField:CreateWindow({
    Name = "SCRIPTS INSANOS ",
    LoadingTitle = " FloppHub Team",
    LoadingSubtitle = "By RTZ_ANDRUUUX ",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "FloppHub",
        FileName = "FloppHub Script"
    },
    Discord = {
        Enabled = true,
        Invite = "J9dSv7hq55",
        RememberJoins = false
    }
})

local function generateDescription(scriptName)
    return "EXECUTE " .. scriptName
end

local Tab0 = Window:CreateTab("DISCORD SERVER", 4483362458)
Tab0:CreateButton({
    Name = "FloppHub Team  (Click To Copy The Invitation Link)",
    Callback = function()
        setclipboard("https://dsc.gg/flopphub-team")
    end
})

Tab0:CreateButton({
    Name = "CHANGE UI (SWITCH TO UI 1.0)",
    Callback = function()
        local settings = { LoadingScreen = true }

local scriptContent = game:HttpGet("https://raw.githubusercontent.com/FloppHub-Team/Script-Hub/refs/heads/main/RedzLib%20Version", true)

if not settings.LoadingScreen then
    scriptContent = scriptContent:gsub(
        "local StarterGui%s-=%s-game:GetService%(\"StarterGui\"%)[^}]+closeConsole%(%)",
        ""
    )
end

loadstring(scriptContent)()
    end
})

