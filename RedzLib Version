local Tab4 = Window:MakeTab({"‚öôÔ∏è‚îÉMISC"})

local jumpSlider = nil
local loopConnection = nil
local OriginalMaxZoomDistance = game.Players.LocalPlayer.CameraMaxZoomDistance
local loopSpeed = false
local speedSlider = nil

Tab4:AddToggle({
    Name = "CAMBIAR VELOCIDAD",
    Default = false,
    Size = UDim2.new(0, 500, 0, 50),
    Callback = function(state)
        loopSpeed = state
        local player = game.Players.LocalPlayer
        local character = player.Character
        local humanoid = character and character:FindFirstChildOfClass("Humanoid")
        
        if humanoid then
            if state then
                if not speedSlider then
                    speedSlider = Tab4:AddSlider({
                        Name = "üèÉ‚îÉVELOCIDAD",
                        Min = 1,
                        Max = 1000,
                        Default = humanoid.WalkSpeed,
                        Size = UDim2.new(0, 500, 0, 50),
                        Callback = function(value)
                            humanoid.WalkSpeed = value
                        end,
                        Smooth = true,
                        Animation = {
                            Enabled = true,
                            Time = 0.5
                        }
                    })
                end
            else
                humanoid.WalkSpeed = 30
                if speedSlider then
                    speedSlider:Destroy()
                    speedSlider = nil
                end
            end
        end
    end
})

game:GetService("RunService").RenderStepped:Connect(function()
    if loopSpeed and speedSlider then
        local player = game.Players.LocalPlayer
        local character = player.Character
        local humanoid = character and character:FindFirstChildOfClass("Humanoid")
        
        if humanoid then
            humanoid.WalkSpeed = speedSlider:GetValue()
        end
    end
end)

local Toggle1 = Tab4:AddToggle({
  Name = "MODIFICAR VELOCIDAD V2",
  Description = "A VECES LA PRIMERA OPCI√ìN NO SIRVE",
  Default = false
})

Tab4:AddSlider({
  Name = "üèÉ‚îÉMODIFICAR VELOCIDAD",
  Min = 1,
  Max = 1000,
  Increase = 10,
  Default = 30,
  Callback = function(Value)
  game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
  end
})

Tab4:AddToggle({
    Name = "CAMBIAR PODER DEL SALTO",
    Description = "CAMBIAR EL PODER DE SALTO",
    Default = false,
    Size = UDim2.new(0, 500, 0, 50),
    Callback = function(state)
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        
        if humanoid then
            if state then
                if not jumpSlider then
                    jumpSlider = Tab4:AddSlider({
                        Name = "ü¶ò‚îÉPODER DE SALTO",
                        Min = 1,
                        Max = 1000,
                        Default = humanoid.JumpPower,
                        Size = UDim2.new(0, 500, 0, 50),
                        Callback = function(value)
                            humanoid.JumpPower = value
                        end,
                        Smooth = true,
                        Animation = {
                            Enabled = true,
                            Time = 0.5
                        }
                    })
                end
            else
                humanoid.JumpPower = 50
                if jumpSlider then
                    jumpSlider:Destroy()
                    jumpSlider = nil
                end
            end
        end
    end
})

Tab4:AddButton({
    Name = "FLY JUMP",
    Description = generateDescription("FLY JUMP"),
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/FloppHub-Team/Fly-Jump/refs/heads/main/Script",true))()
    end
})

Tab4:AddToggle({
    Name = "SALTOS INFINITOS",
    Description = "ACTIVAR/DESACTIVAR LOS SALTOS INFINITOS",
    Default = false,
    Size = UDim2.new(0, 500, 0, 50),
    Callback = function(state)
        local player = game.Players.LocalPlayer
        local character = player.Character
        local humanoid = character.Humanoid

        if state then
            humanoid.JumpPower = 100
            humanoid.AutoRotate = true

            while true do
                wait(0.1)
                if humanoid.Jump == true then
                    character.HumanoidRootPart.Velocity = Vector3.new(0, 50, 0)
                end
            end
        else
            humanoid.JumpPower = 50
            humanoid.AutoRotate = true
        end
    end
})

Tab4:AddToggle({
  Name = "LOOP FULL BRIGHT",
  Description = "ACTIVAR PARA QUE SIEMPRE SEA DE DIA",
  Default = false,
  Callback = function(Value)
    if Value then
      loopConnection = game:GetService("RunService").Stepped:Connect(function()
        game.Lighting.ClockTime = 14
      end)
    else
      if loopConnection then
        loopConnection:Disconnect()
        loopConnection = nil
      end
    end
  end
})

local UnlimitedZoomToggle = Tab4:AddToggle({
  Name = "NO LIMIT ZOOM",
  Description = "ALEJAR LA CAMARA INFINITAMENTE",
  Default = false,
  Callback = function(Value)
    if Value then
      while Value do
        game.Players.LocalPlayer.CameraMaxZoomDistance = game.Players.LocalPlayer.CameraMaxZoomDistance + 999999999999999999999999999999999999999999999999999999
        wait(0.1)
      end
    else
      game.Players.LocalPlayer.CameraMaxZoomDistance = OriginalMaxZoomDistance
    end
  end
})

